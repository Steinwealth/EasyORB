# === OPTIMIZED Development Environment Overrides ===
# High-performance development settings with debugging capabilities
# Last updated: 2025-01-27

# === ENVIRONMENT ===
ENVIRONMENT=development
DEBUG_MODE=true
MOCK_DATA=false

# === OPTIMIZED DEVELOPMENT SETTINGS ===
LOG_LEVEL=DEBUG
SESSION_LOG_LEVEL=DEBUG
LOG_DATA_CALLS=true
LOG_CALL_USAGE=true
LOG_LATENCY_METRICS=true
LOG_PROVIDER_SWITCHES=true
LOG_CACHE_HITS=true

# Performance logging
LOG_PERFORMANCE_METRICS=true
LOG_MEMORY_USAGE=true
LOG_CACHE_EFFICIENCY=true
LOG_CONNECTION_POOL_STATUS=true
LOG_WORKER_STATUS=true

# === OPTIMIZED DEVELOPMENT DATA PROVIDERS ===
ETRADE_SANDBOX=true
POLYGON_QPM_BUDGET=100
ALPHA_VANTAGE_DAILY_LIMIT=100

# Development provider optimization
ETRADE_CONNECTION_POOL_SIZE=5
ALPHA_VANTAGE_CONNECTION_POOL_SIZE=3
YAHOO_FINANCE_CONNECTION_POOL_SIZE=10

# === OPTIMIZED DEVELOPMENT PERFORMANCE ===
MAX_WORKERS=4
BATCH_SIZE=10
CACHE_TTL_SECONDS=30
MEMORY_LIMIT_MB=1024

# Development-specific optimizations
ENABLE_ASYNC_PROCESSING=true
ENABLE_CONNECTION_POOLING=true
ENABLE_MEMORY_OPTIMIZATION=true
ENABLE_CACHE_OPTIMIZATION=true
ENABLE_PARALLEL_PROCESSING=true

# Memory management
MEMORY_CLEANUP_INTERVAL_SECONDS=180
MEMORY_PRESSURE_THRESHOLD=0.7
MEMORY_GC_FORCE_INTERVAL_SECONDS=300

# Caching optimization
CACHE_SIZE_LIMIT=1000
CACHE_HIT_RATE_TARGET=80
CACHE_CLEANUP_THRESHOLD=0.8
CACHE_EVICTION_POLICY=LRU

# === OPTIMIZED DEVELOPMENT ALERTING ===
# SECURITY: Credentials loaded from secretsprivate/telegram.env (local development)
TELEGRAM_CHAT_ID=
ALERT_ON_ERRORS=true
ALERT_ON_DATA_FAILURES=true
ALERT_ON_PERFORMANCE_ISSUES=true
ALERT_ON_MEMORY_USAGE=true
ALERT_ON_CACHE_EFFICIENCY=true

# Development alerting optimization
ALERT_FREQUENCY_LIMIT=60
ALERT_BATCH_SIZE=5
ALERT_PRIORITY_QUEUE=true

# === OPTIMIZED DEVELOPMENT MONITORING ===
MONITOR_CALL_USAGE=true
MONITOR_LATENCY=true
MONITOR_PROVIDER_STATUS=true
MONITOR_CACHE_EFFICIENCY=true
MONITOR_MEMORY_USAGE=true
MONITOR_CPU_USAGE=true
MONITOR_WORKER_STATUS=true

# Monitoring optimization
MONITORING_INTERVAL_SECONDS=15
MONITORING_BATCH_SIZE=10
MONITORING_CACHE_TTL=60
MONITORING_MEMORY_LIMIT_MB=64

# === OPTIMIZED DEVELOPMENT SAFETY ===
AUTOMATION_MODE=off
NO_LIVE_TRADING=true
SIMULATION_MODE=true

# Development safety optimization
SAFETY_CHECKS=true
KILL_SWITCH_ENABLED=true
EMERGENCY_STOP_LOSS_PCT=10.0
MAX_DAILY_TRADES=5

# === DEVELOPMENT TESTING CONFIGURATION ===
ENABLE_UNIT_TESTS=true
ENABLE_INTEGRATION_TESTS=true
ENABLE_PERFORMANCE_TESTS=true
ENABLE_STRESS_TESTS=false

# Testing optimization
TEST_CACHE_TTL=10
TEST_MEMORY_LIMIT_MB=256
TEST_BATCH_SIZE=5
TEST_PARALLEL_WORKERS=2

# === DEVELOPMENT DEBUGGING CONFIGURATION ===
ENABLE_DEBUG_LOGGING=true
ENABLE_PROFILING=true
ENABLE_MEMORY_PROFILING=true
ENABLE_CPU_PROFILING=true
ENABLE_CACHE_PROFILING=true

# Debugging optimization
DEBUG_CACHE_TTL=5
DEBUG_MEMORY_LIMIT_MB=128
DEBUG_BATCH_SIZE=1
DEBUG_PARALLEL_WORKERS=1

# === DEVELOPMENT PERFORMANCE TARGETS ===
TARGET_LATENCY_MS=100
TARGET_CACHE_HIT_RATE=80
TARGET_SUCCESS_RATE=95
TARGET_MEMORY_USAGE_MB=512
TARGET_CPU_USAGE_PCT=50

# Development-specific targets
TARGET_THROUGHPUT_RPS=50
TARGET_ERROR_RATE_PCT=5
TARGET_MEMORY_LEAK_MB=10
TARGET_CPU_SPIKE_PCT=80

# === DEVELOPMENT COST OPTIMIZATION ===
ENABLE_COST_OPTIMIZATION=true
COST_ALERT_THRESHOLD=0.5
COST_AUTO_OPTIMIZATION=true
COST_BUDGET_ALERTS=true

# Development cost targets
TARGET_DAILY_COST_USD=5
TARGET_MONTHLY_COST_USD=100
COST_MONITORING_INTERVAL_SECONDS=3600

# === DEVELOPMENT EMERGENCY CONFIGURATION ===
ENABLE_EMERGENCY_MODE=true
EMERGENCY_MODE_TRIGGER_THRESHOLD=0.8
EMERGENCY_MODE_SIMPLIFIED_PROCESSING=true
EMERGENCY_MODE_REDUCED_MEMORY=true

# Emergency optimization
EMERGENCY_CACHE_CLEAR=true
EMERGENCY_CONNECTION_RESET=true
EMERGENCY_LOGGING_LEVEL=WARNING
EMERGENCY_ALERT_FREQUENCY=5
